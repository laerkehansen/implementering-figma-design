---
import "../styles/global.css";
import BaseLayout from "../layouts/BaseLayout.astro";
import "../styles/reset.css";
import { Image } from "astro:assets";
import EmployeeSection from "../components/EmployeeCard.astro";
import EmployeeCard from "../components/EmployeeCard.astro";
import EmployeeContainer from "../components/EmployeeContainer.astro";
import Login from "../components/Login.astro";
import Circles from "../components/Circles.astro";
import HeaderPhoto from "../components/HeaderPhoto.astro";
import Newsletter from "../components/Newsletter.astro";
import Section1Index from "../components/Section1index.astro";
import FinancialProjections from "../components/FinancialProjections.astro";
import Yellowcircles from "../components/Yellowcircles.astro";
import CoreValues from "../components/Core-values.astro";
import { Icon } from "astro-icon/components";
import ContactUs from "../components/ContactUs.astro";
import Greybox from "../components/Greybox.astro";
import IdeasIntoReality from "../components/IdeasIntoReality.astro";

const employeeData = await fetch(
  "https://raw.githubusercontent.com/daviatkea/frontend-design/main/resources/json/employees.json"
).then((res) => res.json());

const coreValuesdata = await fetch(
  "https://raw.githubusercontent.com/daviatkea/frontend-design/main/resources/json/corevalues.json"
).then((res) => res.json());

const projectionsdata = await fetch(
  "https://raw.githubusercontent.com/daviatkea/frontend-design/main/resources/json/projections.json"
).then((res) => res.json());

const title = "hej";

const images = import.meta.glob<{ default: ImageMetadata }>(
  "/src/assets/images/*.{jpeg,jpg,png,gif}"
);
---

<BaseLayout title={title}>
  <HeaderPhoto />
  <Section1Index />
  <!-- <EmployeeContainer data={employeeData} /> -->
  <FinancialProjections />
  <Yellowcircles />
  <CoreValues theme="darkMode" />
  <ContactUs />
  <IdeasIntoReality />
</BaseLayout>
